{
  "name": "Grey",
  "dark": false,
  "author": "Olga Berdnikova",

  "editorScheme": "/themes/greyScheme.xml",

  "ui": {
    "*": {
      "background": "#F2F3F5",
      "foreground": "#1F1F1F",
      "selectionBackground":  "#5880CC"
    },

    "Borders": {
      "color": "#E6E6E6",
      "ContrastBorderColor": "#E6E6E6"
    },

    "Button.darcula": {
      "defaultStartColor": "#5B85C7",
      "defaultEndColor": "#5581CF",
      "shadowColor": "#A6A6A620",
      "defaultShadowColor": "#A6A6A650",
      "defaultFocusedOutlineColor": "#A9C9F5"
    },

    "EditorPane.inactiveBackground": "#F2F3F5 ",

    "EditorTabs": {
      "active.background": "#FFFFFF",
      "inactive.maskColor": "#666E8012",
      "borderColor": "#E6E6E6"
    },

    "Focus.borderColor": "#95BCF5",

    "Group.separatorColor": "#E6E6E6",

    "link": {
      "foreground": "#2A66BF",
      "hover.foreground": "#2A66BF",
      "pressed.foreground": "#2A66BF",
      "visited.foreground": "#2A66BF"
    },

    "NavBar.borderColor": "#E6E6E6",

    "PasswordField.background": "#FFFFFF",

    "Popup": {
      "Toolbar.Border.color": "#E6E6E6",
      "separatorColor": "#E6E6E6",
      "Header.activeBackground": "#EBEBEB",
      "Header.inactiveBackground": "#EBEBEB"
    },

    "ProgressBar": {
      "progressColor": "#5880CC",
      "indeterminateStartColor": "#c2d4f2",
      "indeterminateEndColor": "#5880CC"
    },

    "SearchEverywhere":  {
      "SearchField.background": "#FFFFFF",
      "Tab.active.background": "#D5D6D7",
      "Advertiser.foreground": "#999999"
    },

    "Separator.foreground": "#E6E6E6",

    "SidePanel.background": "#F2F3F5",

    "StatusBar.borderColor": "#E6E6E6",

    "TabbedPane": {
      "selectedColor": "#5581CF",
      "hoverColor": "#E0E1E3"
    },

    "TextArea.background": "#FFFFFF",
    "TextField.background": "#FFFFFF",

    "ToolWindow": {
      "header": {
        "active.background": "#E6E7E8",
        "border.background": "#E6E6E6",
        "tab.selected": {
          "background": "#E0E1E3",
          "active.background": "#D5D6D7"
        }
      },
      "Button": {
        "selectedBackground": "#D5D6D7",
        "hoverBackground": "#D5D6D7"
      }
    },

    "Tree.rowHeight": 22

  }
}