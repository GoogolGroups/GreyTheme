{
  "name": "Grey",
  "dark": false,
  "author": "Olga Berdnikova",

  "editorScheme": "/themes/greyScheme.xml",

  "ui": {
    "*": {
      "background": "#F2F3F5",
      "foreground": "#1F1F1F",
      "selectionBackground":  "#4372E0"
    },

    "Borders": {
      "color": "#E6E6E6",
      "ContrastBorderColor": "#E6E6E6"
    },

    "Button.darcula": {
      "defaultStartColor": "#5B81C7",
      "defaultEndColor": "#547DD1"
    },

    "EditorPane.inactiveBackground": "#F2F3F5 ",

    "EditorTabs": {
      "active.background": "#FFFFFF",
      "inactive.maskColor": "#666E8012",
      "borderColor": "#E6E6E6"
    },

    "Group.separatorColor": "#E6E6E6",

    "link": {
      "foreground": "#4372E0"
    },

    "NavBar.borderColor": "#E6E6E6",

    "PasswordField.background": "#FFFFFF",

    "Popup": {
      "Toolbar.Border.color": "#E6E6E6",
      "separatorColor": "#E6E6E6",
      "Header.activeBackground": "#EBEBEB",
      "Header.inactiveBackground": "#EBEBEB"
    },

    "SearchEverywhere":  {
      "SearchField.background": "#FFFFFF",
      "Tab.active.background": "#D5D6D7",
      "Advertiser.foreground": "#999999"
    },

    "Separator.foreground": "#E6E6E6",

    "SidePanel.background": "#F2F3F5",

    "StatusBar.borderColor": "#E6E6E6",

    "TextArea.background": "#FFFFFF",
    "TextField.background": "#FFFFFF",

    "ToolWindow": {
      "header": {
        "active.background": "#E6E7E8",
        "border.background": "#E6E6E6",
        "tab.selected": {
          "background": "#E0E1E3",
          "active.background": "#D5D6D7"
        }
      },
      "Button.selectedBackground": "#D5D6D7"
    },

    "Tree.rowHeight": 22

  }
}