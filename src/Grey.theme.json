{
  "name": "Grey",
  "dark": false,
  "author": "Olga Berdnikova",

  "editorScheme": "/themes/greyScheme.xml",

  "ui": {
    "*": {
      "background": "#F2F3F5",
      "foreground": "#1F1F1F",
      "selectionBackground":  "#407FC7",
      "selectionBackgroundInactive": "#D5D5D5",
      "selectionInactiveBackground": "#D5D5D5"
    },

    "Borders": {
      "color": "#e1e3e6",
      "ContrastBorderColor": "#E1E3E6"
    },

    "Button.darcula": {
      "defaultStartColor": "#528CC7",
      "defaultEndColor": "#4989CC",
      "outlineDefaultStartColor": "#487EB8",
      "outlineDefaultEndColor": "#346DAD",
      "shadowColor": "#A6A6A620",
      "defaultShadowColor": "#A6A6A650",
      "defaultFocusedOutlineColor": "#A9C9F5"
    },

    "EditorPane.inactiveBackground": "#F2F3F5 ",

    "EditorTabs": {
      "active.background": "#FFFFFF",
      "inactive.maskColor": "#66738012",
      "borderColor": "#E1E3E6"
    },

    "Focus.borderColor": "#84B8F0",

    "Group.separatorColor": "#E1E3E6",

    "link": {
      "foreground": "#2163A6",
      "hover.foreground": "#2163A6",
      "pressed.foreground": "#2163A6",
      "visited.foreground": "#2163A6"
    },

    "NavBar.borderColor": "#E1E3E6",

    "PasswordField.background": "#FFFFFF",

    "Popup": {
      "Toolbar.Border.color": "#E1E3E6",
      "separatorColor": "#E1E3E6",
      "Separator.foreground": "#999999",
      "Header": {
        "activeBackground": "#E1E2E3",
        "inactiveBackground": "#E1E2E3"
      },
      "Advertiser": {
        "background": "#F2F3F5",
        "foreground": "#999999"
      }
    },

    "ProgressBar": {
      "trackColor": "#D1D1D1",
      "progressColor": "#1E82E6",
      "indeterminateStartColor": "#91C5F2",
      "indeterminateEndColor": "#1E82E6"
    },

    "SearchEverywhere":  {
      "SearchField": {
        "background": "#FFFFFF",
        "borderColor": "#E1E3E6"
      },
      "Tab.active.background": "#D5D6D7",
      "Advertiser.foreground": "#999999"
    },

    "Separator.foreground": "#E1E3E6",

    "SidePanel.background": "#F2F3F5",

    "StatusBar.borderColor": "#E1E3E6",

    "TabbedPane": {
      "selectedColor": "#407FC7",
      "hoverColor": "#E1E2E3"
    },

    "TableHeader.cellBorder": "3,0,3,0",

    "TextArea.background": "#FFFFFF",
    "TextField.background": "#FFFFFF",

    "ToolWindow": {
      "header": {
        "active.background": "#E6E7E8",
        "border.background": "#E1E3E6",
        "tab.selected": {
          "background": "#e1e2e3",
          "active.background": "#D5D6D7"
        },
      "tab.hovered": {
          "background": "#e1e2e3",
          "active.background": "#D5D6D7"
        }
      },
      "Button": {
        "selectedBackground": "#D5D6D7",
        "hoverBackground": "#D5D6D7"
      }
    },

    "Tree.rowHeight": 22

  }
}