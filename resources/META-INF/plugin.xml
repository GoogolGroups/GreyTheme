<idea-plugin>
  <id>com.jetbrains.grayTheme</id>
  <name>Gray Theme</name>
  <version>1.0</version>
  <vendor email="olga.berdnikova@jetbrains.com" url="">Olga Berdnikova</vendor>

  <description><![CDATA[
  A low-contrast light theme with lots of space between text lines. Made for writing in Markdown inside IntelliJ IDEs.<br/><br/>
  Please note that this theme overrides your font settings. Uncheck "Use color scheme font instead of the default" in Setting | Editor | Color Scheme | Color Scheme Font to return your font settings.
      ]]></description>

  <change-notes><![CDATA[
      ]]>
  </change-notes>

  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
  <idea-version since-build="191.0"/>

  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
       on how to target different products -->
  <!-- uncomment to enable plugin in all products
  <depends>com.intellij.modules.lang</depends>
  -->

  <extensions defaultExtensionNs="com.intellij">
    <!-- Add your extensions here -->
    <themeProvider id="dfdeb71c-cf0a-490d-8983-dfb56b5e043f" path="/Gray.theme.json"/>
  </extensions>

  <actions>
    <!-- Add your actions here -->
  </actions>

</idea-plugin>